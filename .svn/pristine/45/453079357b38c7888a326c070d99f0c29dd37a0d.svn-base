package controller;

import view.HMI;
import model.Core;

/**
 * Implementation of Command Interface to call the cut function in the model(core)
 */
public class Cut implements Command{

	private Core c;
	private HMI h;
	
	public Cut(Core c, HMI h) {
		this.c = c;
		this.h = h;
	}
	
	@Override
	public void execute() {
		c.cut();
	}

}

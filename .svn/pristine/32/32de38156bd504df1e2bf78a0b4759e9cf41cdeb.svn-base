package controller;

import view.HMI;
import model.Core;

/**
 * Implementation of Command Interface to call the select function in the model(core)
 */
public class Copy implements Command{

	private Core c;
	private HMI h;
	
	public Copy(Core c, HMI h) {
		this.c = c;
		this.h = h;
	}

	@Override
	public void execute(){
		c.copy();
	}
}
